{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: ['config:recommended'],
  labels: ['dependencies'],
  customManagers: [
    {
      customType: 'regex',
      fileMatch: ['^docker-bake\\.hcl$'],
      matchStrings: [
        'VERSION_NATS\\s*=\\s*"(?<currentValue>[^"]+)"'
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'nats-io/natscli',
      extractVersionTemplate: '^v(?<version>.+)$'
    },
    {
      customType: 'regex',
      fileMatch: ['^docker-bake\\.hcl$'],
      matchStrings: [
        'VERSION_NATS_TOP\\s*=\\s*"(?<currentValue>[^"]+)"'
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'nats-io/nats-top',
      extractVersionTemplate: '^v(?<version>.+)$'
    },
    {
      customType: 'regex',
      fileMatch: ['^docker-bake\\.hcl$'],
      matchStrings: [
        'VERSION_NSC\\s*=\\s*"(?<currentValue>[^"]+)"'
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'nats-io/nsc',
      extractVersionTemplate: '^v(?<version>.+)$'
    },
    {
      customType: 'regex',
      fileMatch: ['^docker-bake\\.hcl$'],
      matchStrings: [
        'VERSION_NKEYS\\s*=\\s*"(?<currentValue>[^"]+)"'
      ],
      datasourceTemplate: 'github-releases',
      depNameTemplate: 'nats-io/nkeys',
      extractVersionTemplate: '^v(?<version>.+)$'
    }
  ],
  packageRules: [
    {
      matchManagers: ['custom.regex'],
      matchFileNames: ['docker-bake.hcl']
    }
  ]
}
